    "sqlite3": "^4.0.6",
    "ts-node": "^8.1.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3",
    "webpack-sentry-plugin": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*{ts,tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
