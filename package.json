    "@types/next": "^8.0.3",
    "@types/node": "^11.13.4",
    "@zeit/next-typescript": "^1.1.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "micro-dev": "^3.0.0",
    "prettier": "^1.17.0",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "typescript": "^3.4.3"
  },
  "husky": {
