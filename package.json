    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "^0.0.3",
    "emotion": "^10.0.9",
    "fastify": "^2.2.0",
    "fastify-nextjs": "^4.1.0",
    "knex": "^0.16.5",
    "konva": "^3.2.5",
    "next": "^8.0.4",
    "pg": "^7.10.0",
    "polished": "^3.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-ga": "^2.5.7",
    "react-konva": "^16.8.6"
  },
  "devDependencies": {
    "@sentry/webpack-plugin": "^1.6.2",
    "@types/knex": "^0.16.1",
