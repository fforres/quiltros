{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": ["migrations/*", "*.js"]
  },
  "rules": {
    "interface-name": false,
    
    "no-namespace": false,
    "jsx-boolean-value": false,
    "member-access": [true, "no-public"],
    "no-empty-interface": false,
    "no-console": false,
    "prettier": true,
    "quotemark": [true, "single"]
  }
}
